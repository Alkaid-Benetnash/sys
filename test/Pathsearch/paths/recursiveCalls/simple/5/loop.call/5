%simple_12 = load  i32, i32* %simple_4, align 4 
enter-call simple
%simple_0 = bitcast i32 %simple_12 to i32 
%simple_2 = alloca i32, align 4 
%simple_3 = alloca i32, align 4 
%simple_4_1 = alloca i32, align 4 
